    "@opam/ppx_deriving": "*",
    "@opam/ppx_pipebang": "*",
    "@opam/ppx_deriving_yojson": "*",
    "@opam/ppx_let": "*",
    "@opam/yojson": "*",
    "@esy-ocaml/reason": "*",
    "refmterr": "*",
    "@reason-native/rely": "*",
    "rench": "*"
  },
  "resolutions": {
    "@esy-ocaml/reason": "facebook/reason#ab49908",
    "@opam/menhir": "20171013",
    "@opam/re": "1.7.3",
    "rench": "bryphe/rench#980c38f"
  },
  "devDependencies": {
    "@opam/merlin": "^3.2.2",
    "ocaml": "~4.6.0"
